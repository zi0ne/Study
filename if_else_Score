function convertScoreToGrade(score) {
  if(100>= score && 90 <= score){
    return 'A'
  }
  if(89>= score && 80 <= score){
    return 'B'
  }
  if(79>= score && 70 <= score){
    return 'C'
  }
  if(69>= score && 60 <= score){
    return 'D'
  }
  if(59>= score && 0 <= score){
    return 'F'
  }
  else{
    return 'INVALID SCORE'
  }
}
